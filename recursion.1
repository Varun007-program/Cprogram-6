#include <stdio.h>
#include <conio.h>
 
int s(int n) 
{ 
    if (n == 0) 
       return 0;
	   else 
    return (n % 10 + s(n / 10)); 
} 

int main() 
{ 
    int n;
	printf("Enter the number :\n");
	scanf("%d",&n); 
    int result = s(n); 
    printf("Sum of digits in %d is %d\n", n, result); 
    return 0; 
} 
